{
  "name": "@whppt/next",
  "version": "0.0.2",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "/dist",
    "/types",
    "/styles"
  ],
  "repository": "https://github.com/whpptjs/whppt-next.git",
  "author": "Svelte Team <info@sveltestudios.com>",
  "keywords": [
    "CMS",
    "Next",
    "React"
  ],
  "license": "MIT",
  "homepage": "https://github.com/whpptjs/whppt-next",
  "bugs": {
    "url": "https://github.com/whpptjs/whppt-next/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "watch": "run-p watch:*",
    "watch:js": "tsc --watch",
    "watch:css": "postcss src/styles/index.scss -o styles/base.css -w",
    "build": "run-p build:*",
    "build:js": "rm -rf dist && rm -rf types && tsc",
    "build:css": "NODE_ENV=production postcss src/styles/index.scss -o styles/base.css",
    "publish:patch": "yarn test && yarn build && standard-version --release-as patch && yarn publish && git push --follow-tags",
    "publish:minor": "yarn test && yarn build && standard-version --release-as minor && yarn publish && git push --follow-tags",
    "publish:major": "yarn test && yarn build && standard-version --release-as major && yarn publish && git push --follow-tags"
  },
  "dependencies": {
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.14",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.12",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.1.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.3"
  }
}
